{
  "name": "inventory",
  "version": "1.0.0",
  "description": "sample nodejs api",
  "main": "index.js",
  "scripts": {
    "test": "node test",
    "coverage": "jest --coverage --verbose test",
    "start": "node index.js",
    "eslint": "eslint . ",
    "pretest": "eslint . && npm run coverage"
  },
  "author": "Lalitha M",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.309.0",
    "aws-sdk-mock": "^5.8.0",
    "connect": "^3.2.0",
    "js-yaml": "^3.12.0",
    "swagger-tools": "^0.10.4"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "jest": "^23.4.2",
    "supertest": "^1.2.0",
    "tape": "^4.5.1"
  },
  "jest": {
    "testRegex": ".*unit-test\\.js$",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "app/**/*.js",
      "!**/node_modules/**"
    ],
    "coverageDirectory": "./coverage/unit-test",
    "verbose": true
  },
  "directories": {
    "test": "test"
  }
}
